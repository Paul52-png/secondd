import random
def угадай_число():
    загаданное_число = random.randint(0, 10)
    попытки = 3
    print("угадай число от 0 до 10 у тебя 3 попытки")
    while попытки > 0:
        попытка = int(input("твое число: "))
        if попытка == загаданное_число:
            print("ты угадал")
            break
        elif попытка < загаданное_число:
            print("больше")
        else:
            print("меньше")
        попытки -= 1
    if попытки == 0:
        print(f"ты проиграл загаданное число было: {загаданное_число}")
угадай_число
